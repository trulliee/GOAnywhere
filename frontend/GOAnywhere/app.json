{
  "expo": {
    "name": "GOAnywhere",
    "slug": "GOAnywhere",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/GOAnywhere.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.trolley.GOAnywhere"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.trolley.GOAnywhere",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDHIQoHjcVR0RsyKG-U5myMIpdPqK6n-m0"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "https://api.mapbox.com/downloads/v2/releases/maven",
              "https://oss.sonatype.org/content/repositories/snapshots",
              "https://jitpack.io"
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0ba53dbd-9393-4008-917d-ad058e25fd8b"
      }
    }
  }
}
